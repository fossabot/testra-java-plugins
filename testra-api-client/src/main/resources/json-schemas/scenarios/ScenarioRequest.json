{
  "$id": "http://example.com/example.json",
  "type": "object",
  "definitions": {},
  "$schema": "http://json-schema.org/draft-06/schema#",
  "properties": {
    "name": {
      "$id": "/properties/name",
      "type": "string",
      "title": "The Name Schema ",
      "default": "",
      "examples": [
        "Scenario-2"
      ]
    },
    "featureName": {
      "$id": "/properties/featureName",
      "type": "string",
      "title": "The Featurename Schema ",
      "default": "",
      "examples": [
        "Feature 2"
      ]
    },
    "featureDescription": {
      "$id": "/properties/featureDescription",
      "type": "string",
      "title": "The Featuredescription Schema ",
      "default": "",
      "examples": [
        "Feature desc"
      ]
    },
    "backgroundSteps": {
      "$id": "/properties/backgroundSteps",
      "type": "array",
      "items": {
        "$id": "/properties/backgroundSteps/items",
        "type": "object",
        "properties": {
          "index": {
            "$id": "/properties/backgroundSteps/items/properties/index",
            "type": "integer",
            "title": "The Index Schema ",
            "default": 0,
            "examples": [
              0
            ]
          },
          "text": {
            "$id": "/properties/backgroundSteps/items/properties/text",
            "type": "string",
            "title": "The Text Schema ",
            "default": "",
            "examples": [
              "Step-2"
            ]
          }
        }
      }
    },
    "steps": {
      "$id": "/properties/steps",
      "type": "array",
      "items": {
        "$id": "/properties/steps/items",
        "type": "object",
        "properties": {
          "index": {
            "$id": "/properties/steps/items/properties/index",
            "type": "integer",
            "title": "The Index Schema ",
            "default": 0,
            "examples": [
              0
            ]
          },
          "text": {
            "$id": "/properties/steps/items/properties/text",
            "type": "string",
            "title": "The Text Schema ",
            "default": "",
            "examples": [
              "Step-2"
            ]
          }
        }
      }
    },
    "tags": {
      "$id": "/properties/tags",
      "type": "array",
      "items": {
        "$id": "/properties/tags/items",
        "type": "string",
        "title": "The 0th Schema ",
        "default": "",
        "examples": [
          "@player",
          "@backoffice"
        ]
      }
    }
  }
}